public Boolean isPrime(Integer num) {
    //code 
    Boolean isPrime = false;
    if(num == null || num <= 1){
        return null;
    }
     for(Integer i=2; i <= Math.sqrt(num); i++){

        if (Math.mod(num,i) == 0){
            isPrime = false;
            break;
    }else{
        isPrime = true;

    }
   
}
     return isPrime;
}