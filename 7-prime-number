public Boolean isPrime(Integer num) {
    //code 
    //Boolean isPrime = false;
    if(num == null || num <= 1){
        return null;
    }
     for(Integer i=2; i <= Math.floor(Math.sqrt(num)).intValue(); i++){

        if (Math.mod(num,i) == 0){
            return false;
            
    }
   
}
     return true;
}