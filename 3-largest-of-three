public static Integer findLargest(Integer num1, Integer num2, Integer num3) {
	//code here
    if (num1 == null || num2 ==  null || num3 == null)
        retrun null;

    if(num1 > num2 ){
        if(num1 > num3)
            retrun num1;
    }
    else if(num2 > num1){
        if(num2 > num3)
         return num2;
    }
    else {
        return num3;
    }

}